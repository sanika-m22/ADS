#include <stdio.h>
int main() {
    int key,i,n,a[50],m,l,u;
    printf("enter the no .of elements in array:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
      printf("enter the array element %d:",i+1);
      scanf("%d",&a[i]);
    }
    printf("enter the search key:");
    scanf("%d",&key);
    l=0;
    u=n-1;
    while(l<=u)
    {
        m=(l+u)/2;
        if(a[m]==key)
        {
            printf("element found at %d",m+1);
            return 0;
        }
        else
        {
            if(key<a[m])
            {
                u=m-1;
            }
            else
            {
                l=m+1;
            }
        }
    }
    printf("element not found");
    return 0;
}
