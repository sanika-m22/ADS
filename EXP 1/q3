#include <stdio.h>

int main() {
    int n,a[10],found=0,i,j;
    printf("enter no.of elements:");
    scanf("%d",&n);
    printf("enter the array elements:");
    for(i=0;i<n;i++){
    scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                found=1;
                printf("duplicate is:%d",a[i]);
                break;
            }
        }
        if(found)
        {
            break;
        }
        if(!found)
        {
            printf("no repeat");
        }
    }
     return 0;
   }
