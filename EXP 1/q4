#include <stdio.h>

int main() {
  int a[50],i,n,min,max;
    printf("enter no.of elements:");
    scanf("%d",&n);
    printf("enter the array elements:");
    for(i=0;i<n;i++){
    scanf("%d",&a[i]);
    }
    min=a[0];
    max=a[0];
    for(i=0;i<n;i++){
    if(a[i]>max)
    {
        max=a[i];
    }
    if(a[i]<min)
    {
        min=a[i];
    }
    }
    printf("greatest:%d",max);
    printf("smallest:%d",min);
    return 0;
}
