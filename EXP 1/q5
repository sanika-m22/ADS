#include <stdio.h>

int main() {
    int n,a[50],i;
    printf("enter no.of elements:");
    scanf("%d",&n);
    printf("enter the array elements:");
    for(i=1;i<=n;i++){
    scanf("\n%d",&a[i]);
    }
    for(i=1;i<=n;i++)
    {
        if(i%2!=0)
        {
            a[i]=a[i]*a[i];
        }
    }
    printf("array after:");
    for(i=1;i<=n;i++)
    {
        printf("%d ",a[i]);
    }
    

    return 0;
}
